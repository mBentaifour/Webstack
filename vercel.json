{
    "version": 2,
    "builds": [
        {
            "src": "vercel_app.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "vercel_app.py"
        }
    ],
    "env": {
        "DJANGO_SETTINGS_MODULE": "webstack_django.settings",
        "PYTHONPATH": "/var/task"
    }
}
